<%= simple_form_for(@ticket, :html => { :class => 'simple_form well form-horizontal' })  do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.association :ticket_state %>
    <%= f.association :ticket_type %>
    <%= f.association :ticket_priority %>
    <%= f.input :percent %>
    <%= f.association :project %>
    <%= f.association :user, label_method: :name %>
    <%= f.button :submit, :value => I18n.t(:'simple_form.submit') %>
<% end %>
